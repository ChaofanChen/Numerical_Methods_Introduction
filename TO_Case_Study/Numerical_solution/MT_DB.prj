        <mesh>MT_DB_physical_group_Right_boundary.vtu</mesh>
        <mesh>MT_DB_physical_group_Hauptrogenstein.vtu</mesh>
    </meshes>
    <processes>
        <process>
            <name>MT_DB</name>
            <type>THERMO_HYDRO_MECHANICS</type>
            <integration_order>2</integration_order>
            <constitutive_relation id="0,1,2,3,4,5,6,7">
                <type>LinearElasticIsotropic</type>
                <youngs_modulus>YoungsModuliClay</youngs_modulus>
                <poissons_ratio>PoissonsRatiosClay</poissons_ratio>
            </constitutive_relation>
            <!--Needed: T RH Pw d_x d_y e_x e_y-->
            <process_variables>
                <temperature>temperature</temperature>
                <pressure>pressure</pressure>
                <displacement>displacement</displacement>
            </process_variables>
            <specific_body_force>0 -6.94</specific_body_force>
            <!-- g at 45Â° to bh-->
            <!--specific_body_force>-6.94 -6.94</specific_body_force-->
            <initial_stress type="total">sigma_ic</initial_stress>
        </process>
    </processes>
