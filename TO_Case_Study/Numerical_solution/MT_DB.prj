<OpenGeoSysProject>
    <meshes>
        <mesh>MT_DB_domain.vtu</mesh>
        <mesh>MT_DB_physical_group_Upper_boundary.vtu</mesh>
        <mesh>MT_DB_physical_group_Bottom_boundary.vtu</mesh>
